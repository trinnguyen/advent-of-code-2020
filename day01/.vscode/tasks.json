{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"command": "cargo",
			"args": ["run", "samples/test"],
			"problemMatcher": [
				"$rustc"
			],
			"label": "Rust: cargo run test",
			"group": {
				"kind": "build"
			}
		},
		{
			"type": "shell",
			"command": "cargo",
			"args": ["run", "samples/input"],
			"problemMatcher": [
				"$rustc"
			],
			"label": "Rust: cargo run input",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}